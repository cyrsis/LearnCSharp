<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing" x:Class="Shapes.MainWindow"
        Title="MainWindow" Height="350" Width="525">
	<Canvas>
		<Rectangle Width="50"
			Height="50"
			Fill="Blue"
			Canvas.Top="50"
			Canvas.Left="50"
			Stroke="Red"
			StrokeThickness="5"/>
		<Ellipse Width="50"
			Height="50"
			Fill="Green"
			Canvas.Top="50"
			Canvas.Left="150"
			Stroke="Red"
			StrokeThickness="2" />
		<Line X1="0"
			Y1="0"
			X2="25"
			Y2="50"
			Stroke="Red"
			StrokeThickness="10" />
		<Polygon
			Fill="Green"
			Points="100,100 200,150 125,175 175,250"
			Stroke="Blue"
			StrokeThickness="5" />
      
      
		<ed:Arc ArcThickness="1" ArcThicknessUnit="Percent" EndAngle="360" Fill="#FF73BD29" Height="107.481" Canvas.Left="285.37" Stretch="None" Stroke="Black" StartAngle="90" Canvas.Top="83.236" Width="95.079"/>
      
      
	</Canvas>
</Window>
