                                <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="0" />
                                <EasingDoubleKeyFrame KeyTime="0:0:1.6" Value="150" />
                                <EasingDoubleKeyFrame KeyTime="0:0:2.9" Value="200" />
