                                <EasingDoubleKeyFrame KeyTime="0:0:0.9" Value="0" />
                                <EasingDoubleKeyFrame KeyTime="0:0:1.2" Value="1" />
                                <EasingDoubleKeyFrame KeyTime="0:0:3.3" Value="1" />
