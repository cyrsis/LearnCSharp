<Window x:Class="DemoWpfApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <ListBox Name="FirstNum" SelectionChanged="FirstNum_OnSelectionChanged"></ListBox>
            <ListBox Name="SecondNum" SelectionChanged="SecondNum_OnSelectionChanged"></ListBox>
            <Button Name="Divide" Click="Divide_OnClick">Divide</Button>
            <TextBlock Name="Output"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
